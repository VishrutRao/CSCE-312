class Square {
	field int x, y;
	field int size;
	
	constructor Square new(int ax, int ay, int as) {
		let x = ax;
		let y = ay;
		let size = as;
		
		return this;
	}
	
	method void dispose() {
		do Memory.deAlloc(this);
		return;
	}
	
	method void draw() {
		do Screen.setColor(true);
		do Screen.drawRectangle(x, y, x + size, y + size);
		
		return;
	}
}