class Main {
	function void main() {
		var MSSquare ms;
		let ms = MSSquare.new(20, 20, 20);
		do ms.draw();
		do Keyboard.readChar();
		do ms.setShow();
		do ms.draw();
		do Keyboard.readChar();
		do ms.setMine();
		do ms.draw();
		do Keyboard.readChar();
		do ms.unsetMine();
		do ms.setCount(2);
		do ms.draw();
		do Keyboard.readChar();
		
		return;
	}
}